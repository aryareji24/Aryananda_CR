   "source": [
    "n = int(input(\"enter a number: \"))\n",
    "def adds(n):\n",
    "    total = 0\n",
    "    for i in range(1,n+1):\n",
    "        total = total + i\n",
    "    print(total)\n",
    "adds(n)"
